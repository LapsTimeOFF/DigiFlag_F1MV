{
  "name": "digiflag-f1mv",
  "productName": "DigiFlag-F1MV",
  "version": "2.5.0",
  "description": "Adding a DigiFlag functionality to MultiViewer For F1",
  "main": "./dist/main/main.js",
  "license": "MIT",
  "author": "DigiFlag Team",
  "contributors": [
    {
      "name": "LapsTimeOFF",
      "url": "https://github.com/LapsTimeOFF"
    },
    {
      "name": "TheBaconCat",
      "url": "https://github.com/TheBaconCat"
    },
    {
      "name": "JJwatMyself",
      "url": "https://github.com/JJwatMyself"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LapsTimeOFF/DigiFlag_F1MV.git"
  },
  "bugs": {
    "url": "https://github.com/LapsTimeOFF/DigiFlag_F1MV/issues"
  },
  "scripts": {
    "dev": "electron-vite build && electron-vite dev -w ",
    "preview": "electron-vite build && electron-vite preview",
    "build": "npm run typecheck && electron-vite build",
    "build:win": "electron-vite build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-store": "^8.1.0",
    "electron-updater": "^5.3.0",
    "express": "^4.18.2",
    "i18next": "^22.4.10",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-backend": "^2.1.1",
    "ip": "^1.1.8",
    "jquery": "^3.6.3",
    "jquery-i18next": "^1.2.1",
    "npm_f1mv_api": "^1.4.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/express": "^4.17.17",
    "@types/ip": "^1.1.0",
    "@types/jquery": "^3.5.16",
    "@types/node": "18.11.18",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "electron": "^23.1.4",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.2",
    "electron-vite": "^1.0.19",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}